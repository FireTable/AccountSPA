{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"

      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "style": true, "libraryName": "antd-mobile" }]
      ]
    }
  },
  "proxy": {
  "/api": {
    "target": "http://localhost/accountspa_php/public/index.php/api/",
    "changeOrigin": true,
    "pathRewrite": { "^/api" : "" }
    }
  },
  "theme": {

    "@link-color": "#1DA57A",
    "@border-radius-base": "2px",
    "@font-size-base": "24px"

  }
}
